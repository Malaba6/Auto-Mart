{
    "name": "auto-mart",
    "version": "1.0.0",
    "description": "Auto Mart is an online marketplace for automobiles of diverse makes, model or body type. With Auto Mart, users can sell their cars or buy from trusted dealerships or private sellers",
    "main": "app.js",
    "scripts": {
        "test": "nyc --reporter=html --reporter=text --cache mocha ./app/test --require @babel/polyfill --require @babel/register --recursive --no-timeout --exit",
        "coverage": "nyc report --reporter=text-lcov | coveralls",
        "dev-start": "nodemon --exec babel-node --presets @babel/preset-env app.js",
        "dev": "cross-env NODE_ENV=development node --require @babel/register app/database/db createTables && nodemon --exec babel-node --presets @babel/preset-env app.js",
        "start": "babel-node app.js",
        "lint": "eslint --fix app/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Malaba6/Auto-Mart.git"
    },
    "keywords": [
        "auto-mart",
        "cars",
        "sale",
        "car-market",
        "auto",
        "api"
    ],
    "author": "Eric Malaba",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Malaba6/Auto-Mart/issues"
    },
    "homepage": "https://github.com/Malaba6/Auto-Mart#readme",
    "dependencies": {
        "@babel/cli": "^7.4.4",
        "@babel/core": "^7.4.5",
        "@babel/node": "^7.4.5",
        "@babel/plugin-proposal-class-properties": "^7.4.4",
        "@babel/polyfill": "^7.4.4",
        "@babel/preset-env": "^7.4.5",
        "@babel/register": "^7.4.4",
        "chai-http": "^4.3.0",
        "cloudinary": "^1.14.0",
        "cross-env": "^5.2.0",
        "debug": "^4.1.1",
        "dotenv": "^8.0.0",
        "express": "^4.17.0",
        "make-runnable": "^1.3.6",
        "moment": "^2.24.0",
        "nodemon": "^1.19.0",
        "pg": "^7.11.0",
        "swagger-ui-express": "^4.0.6",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "body-parser": "^1.19.0",
        "chai": "^4.2.0",
        "coveralls": "^3.0.3",
        "eslint": "^5.16.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "mocha": "^6.1.4",
        "nyc": "^14.1.1"
    },
    "engines": {
        "node": "10.15.3"
    }
}